{
    "extends": "eslint:recommended",
    "env": {
        "browser": true
    },
    "globals": {
        "_": true,
        "chrome": true,
        "Clipboard": true,
        "localStorage": true,
        "$": true,
        "QUnit": true,
        "require": true,
        "urlNotification": true
    },
    "rules": {
        "indent": ["error", 2],
        "no-console": ["error", { "allow": ["info", "warn", "error"] }],
        "eqeqeq": "error"
    }
}
