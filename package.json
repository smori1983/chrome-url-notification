{
  "name": "chrome-url-notification",
  "version": "5.1.0",
  "description": "Chrome URL Notification",
  "directories": {},
  "scripts": {
    "build:prod": "cross-env EXTENSION_MODE=production gulp build",
    "build:dev": "cross-env EXTENSION_MODE=development gulp build",
    "make:vendor:js": "gulp make:vendor:js",
    "make:urlNotification": "gulp make:urlNotification",
    "make": "gulp make",
    "test": "mocha --require mock-local-storage test/*",
    "test:coverage": "istanbul cover --print both _mocha -- test/* --require mock-local-storage",
    "watch:build:dev": "watch 'npm run build:dev' src/ --interval=1 --ignoreDotFiles",
    "watch:test": "watch 'npm run test' src/ test/ --interval=1 --ignoreDotFiles",
    "lint": "gulp lint",
    "report:coverage": "istanbul cover _mocha -- test/* --require mock-local-storage && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smori1983/chrome-url-notification.git"
  },
  "devDependencies": {
    "@types/chrome": "0.0.83",
    "@types/jquery": "^3.3.31",
    "@types/node": "^12.7.2",
    "browserify": "^16.5.0",
    "codecov": "^3.5.0",
    "cross-env": "^5.2.0",
    "del": "^4.1.1",
    "dotenv": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-eslint": "^5.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^6.2.0",
    "mock-local-storage": "^1.1.8",
    "pump": "^3.0.0",
    "sinon-chrome": "^3.0.1",
    "sprintf-js": "^1.1.2",
    "vinyl-source-stream": "^2.0.0",
    "watch": "^1.0.2"
  },
  "dependencies": {
    "bootstrap": "^3.4.1",
    "bootstrap-colorpicker": "^2.5.3",
    "clipboard": "^2.0.4",
    "deepmerge": "^3.3.0",
    "jquery": "^3.4.1",
    "jquery-validation": "^1.19.1",
    "jsonschema": "^1.2.4",
    "lodash": "^4.17.15"
  }
}
