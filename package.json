{
  "name": "chrome-url-notification",
  "version": "6.3.0",
  "description": "Chrome URL Notification",
  "directories": {},
  "scripts": {
    "build:prod": "webpack",
    "build:dev": "webpack --config webpack.dev.config.js",
    "build:dev:watch": "watch 'npm run build:dev' src/ --interval=1 --ignoreDotFiles",
    "build:test": "webpack --config webpack.test.config.js",
    "license:summary": "license-checker --production --summary --unknown",
    "lint": "eslint src/ test/ test_lib/",
    "lint:fix": "eslint --fix src/ test/ test_lib/",
    "test": "mocha --require mock-local-storage test/*",
    "test:app": "mocha --require mock-local-storage test/app/*",
    "test:urlNotification": "mocha --require mock-local-storage test/urlNotification/*",
    "test:coverage": "istanbul cover --print both _mocha -- test/* --require mock-local-storage",
    "test:watch": "watch 'npm run test' src/ test/ test_lib/ --interval=1 --ignoreDotFiles",
    "test:app:watch": "watch 'npm run test:app' src/ test/ test_lib/ --interval=1 --ignoreDotFiles",
    "test:urlNotification:watch": "watch 'npm run test:urlNotification' src/ test/ test_lib/ --interval=1 --ignoreDotFiles",
    "coverage:report": "istanbul cover _mocha -- test/* --require mock-local-storage && codecov"
  },
  "dependencies": {
    "bootstrap": "^4.6.1",
    "bootstrap-colorpicker": "^3.4.0",
    "clipboard": "^2.0.8",
    "deepmerge": "^4.2.2",
    "joi": "^17.4.2",
    "jquery": "^3.6.0",
    "jsonschema": "^1.4.0",
    "lodash": "^4.17.21",
    "popper.js": "^1.16.1",
    "sprintf-js": "^1.1.2",
    "url": "^0.11.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@types/chrome": "0.0.164",
    "@types/jquery": "^3.5.8",
    "@types/mocha": "^9.0.0",
    "@types/node": "^14.17.33",
    "@types/sinon-chrome": "^2.2.11",
    "babel-loader": "^8.2.3",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "codecov": "^3.8.3",
    "color-js": "^1.0.5",
    "copy-webpack-plugin": "^9.1.0",
    "css-loader": "^6.5.1",
    "eslint": "^7.0.0",
    "istanbul": "^0.4.5",
    "jsdom": "^18.1.0",
    "license-checker": "^25.0.1",
    "mocha": "^9.1.3",
    "mocha-testdata": "^1.2.0",
    "mock-local-storage": "^1.1.17",
    "sass": "^1.43.4",
    "sass-loader": "^12.3.0",
    "sinon-chrome": "^3.0.1",
    "style-loader": "^3.3.1",
    "text-encoder": "0.0.4",
    "watch": "^1.0.2",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smori1983/chrome-url-notification.git"
  },
  "author": "Shinichiro MORI",
  "license": "MIT"
}
